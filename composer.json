        "laravel-package",
        "voyager",
        "vuejs"
    ],
    "license": "MIT",
    "homepage": "https://classiebit.com/eventmie",
    "support": {
        "issues": "https://github.com/classiebit/eventmie/issues",
        "source": "https://github.com/classiebit/eventmie"
